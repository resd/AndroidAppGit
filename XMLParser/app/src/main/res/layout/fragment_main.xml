<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="com.evgen3.xmlparser.app.application.ShowResult$PlaceholderFragment">

    <TextView
        android:id="@+id/speciality"
        android:text="Специальность: "
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/abiturient"
        android:layout_alignParentLeft="true"/>

    <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Степень: "
            android:id="@+id/qualification"
            android:phoneNumber="false"
            android:layout_below="@+id/speciality"
            android:layout_alignParentLeft="true"/>

    <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Доступно мест: "
            android:id="@+id/licence"
            android:layout_below="@+id/qualification"
            android:layout_alignParentLeft="true"/>

    <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Из них бюджетных: "
            android:id="@+id/budget"
            android:layout_below="@+id/licence"
            android:layout_alignParentLeft="true"/>

    <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Ваш рейтинг: "
            android:id="@+id/rating"
            android:layout_below="@+id/budget"
            android:layout_alignParentLeft="true"/>

    <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Привилегии: "
            android:id="@+id/privilege"
            android:layout_below="@+id/rating"
            android:layout_alignParentLeft="true"/>

    <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Empty"
            android:id="@+id/docstate"
            android:layout_below="@+id/privilege"
            android:layout_alignParentLeft="true"/>

    <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Рекомендовано до зарахування на бюджет"
            android:id="@+id/title"
            android:layout_below="@+id/docstate"
            android:layout_alignParentLeft="true"/>

    <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Рекомендация дие до ..."
            android:id="@+id/comment"
            android:layout_below="@+id/title"
            android:layout_alignParentLeft="true"/>

    <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Место в списке: "
            android:id="@+id/place"
            android:layout_below="@+id/comment"
            android:layout_alignParentLeft="true"/>

    <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Место по поданным оригиналам: "
            android:id="@+id/originplace"
            android:layout_below="@+id/place"
            android:layout_alignParentLeft="true"/>

    <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Абитуриент: "
            android:id="@+id/abiturient"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true"/>

    <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/originplaceDX"
            android:layout_alignTop="@+id/originplace"
            android:layout_toRightOf="@+id/originplace"/>

    <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/placeDX"
            android:layout_above="@+id/originplace"
            android:layout_toRightOf="@+id/place"/>

</RelativeLayout>
